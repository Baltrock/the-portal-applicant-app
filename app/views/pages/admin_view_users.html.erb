<div class="benscard">
    <h4>All Applicants:</h4><br>
      <table>
        <tr>
          <th>Profile Picture</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Age</th>
          <th>Nationality</th>
          <th>City</th>
          <th>Phone Number</th>
          <th>Email</th>
          <th>Introduction</th>
          <th>Bio</th>
          <th>Social Media</th>
        </tr>
        <% InformationForm.all.each do |form| %>
          <tr>
            <td><%= form.profile_picture %></td>
            <td><%= form.first_name %></td>
            <td><%= form.last_name %></td>
            <td><%= form.age %></td>
            <td><%= form.nation %></td>
            <td><%= form.city %></td>
            <td><%= form.phone_number %></td>
            <td><%= form.email%></td>
            <td><%= form.introduction %></td>
            <td><%= form.bio %></td>
            <td><%= form.social_media %></td>
          </tr>
        <% end %>
      </table>
  </div>
